package com.exercise4;

public class KeyValuePair{
	private String newKey = "",
									newValue = "",
									currentKey = "",
									currentValue = "";
	
	public KeyValuePair(String newKey, String newValue){
		this.newKey = newKey;
		this.newValue = newValue;
	}
	
	public KeyValuePair(String newKey, String newValue, String currentKey, String currentValue){
		this.newKey = newKey;
		this.newValue = newValue;
		this.currentKey = currentKey;
		this.currentValue = currentValue;
	}
	
	public String getNewKey(){
		return newKey;
	}
	
	public String getNewValue(){
		return newValue;
	}
	
	public String getCurrentKey(){
		return currentKey;
	}
	
	public String getCurrentValue(){
		return currentValue;
	}	
}
